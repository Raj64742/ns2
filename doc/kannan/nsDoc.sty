sub do_cmd_textasciitilde {
    join('', '\~', $_[0]);                      #$
}

#
# Some from the ifthen package
#
sub do_cmd_equal {
    local($_) = @_;                             #$

    s/$next_pair_pr_rx//o;                      #$
    local($a) = $&;
    $a =~ s/$1//g;

    s/$next_pair_pr_rx//o;                      #$
    local($b) = $&;
    $b =~ s/$1//g;

    join('', ($a eq $b ? 1 : 0), $_);
}

sub do_cmd_ifthenelse {
    local($_) = @_;                             #$

    s/$next_pair_pr_rx//o;                      #$
    local($a) = $&;
    $a =~ s/<#$1#>//g;

    s/$next_pair_pr_rx//o;                      #$
    local($then) = $&;

    s/$next_pair_pr_rx//o;                      #$
    local($else) = $&;

    join('', ($a eq 1 ? $then : $else), $_);
}

sub do_cmd_tup {
    local($_) = @_;                             #$
    local($rest) = $_;
    $rest =~ s/$next_pair_pr_rx//o;
    join('', '&lt;', $&, '&gt;', $rest);
}

