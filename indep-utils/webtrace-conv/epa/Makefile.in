CC = @CXX@
MKDEP	= ../../../conf/mkdep

INCLUDE = -I. @V_INCLUDES@
CFLAGS = @V_CCOPT@
LDFLAGS = @V_STATIC@
LIBS = @V_LIB_TCL@ @V_LIB@ @LIBS@

SRC = tr-stat.cc
OBJ = $(SRC:.cc=.o)

all: tr-stat

tr-stat: $(OBJ)
	$(CC) -o $@ $(LDFLAGS) $(CFLAGS) $(INCLUDE) $(OBJ) $(LIBS)

.SUFFIXES: .cc

.cc.o: 
	@rm -f $@
	$(CC) -c $(CFLAGS) $(INCLUDE) -o $@ $*.cc

clean: 
	@rm -f *~ *.o tr-stat core
