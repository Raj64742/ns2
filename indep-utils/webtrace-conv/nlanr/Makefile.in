# Top level hierarchy
prefix  = @prefix@
# Pathname of directory to install the binary
BINDEST = @prefix@/bin

CC = @CXX@
INCLUDE = -I. -I../../.. @V_INCLUDES@
CFLAGS = @V_CCOPT@ -DCPP_NAMESPACE=@CPP_NAMESPACE@
LDFLAGS = @V_STATIC@
LIBS = @V_LIB_TCL@ @V_LIB@ @LIBS@
INSTALL = @INSTALL@

SRC = tr-stat.cc logparse.cc
OBJ = $(SRC:.cc=.o)

all: nlanr-tr-stat

nlanr-tr-stat: $(OBJ)
	$(CC) $(LDFLAGS) $(CFLAGS) -o $@ $(OBJ) $(LIBS)

install: nlanr-tr-stat
	$(INSTALL) -m 555 -o bin -g bin nlanr-tr-stat $(DESTDIR)$(BINDEST)

.SUFFIXES: .cc 

.cc.o: 
	@rm -f $@
	$(CC) -c $(CFLAGS) $(INCLUDE) -o $@ $*.cc

clean: 
	rm -f *.o *~ nlanr-tr-stat *core

depend: $(SRC)
	$(MKDEP) $(CFLAGS) $(INCLUDE) $(SRC)
